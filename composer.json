{
    "name": "pteroca-com/pterodactyl-addon",
    "version": "0.1.2",
    "description": "This package brings an API interface to the application API in order to be able to create API tokens for a user",
    "keywords": [
        "PterodactylApiAddon",
        "laravel",
        "pterodactyl-api-addon"
    ],
    "homepage": "https://pteroca.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Konrad Sroga",
            "email": "konradsroga@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "firebase/php-jwt": "^6.11",
        "paragonie/sodium_compat": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Pteroca\\PterodactylAddon\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pteroca\\PterodactylAddon\\PterodactylApiAddonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "illuminate/http": "^11.42"
    }
}
